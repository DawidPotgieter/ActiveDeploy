<Window x:Class="Active.Builder.Password"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Password" Height="128" Width="300" ShowInTaskbar="False" WindowStartupLocation="CenterOwner" WindowStyle="ToolWindow" Loaded="Window_Loaded">
    <Grid Height="86">
		<Label Content="Password :" Height="28" HorizontalAlignment="Left" Margin="12,15,0,0" Name="lblPassword" VerticalAlignment="Top" />
		<PasswordBox Height="23" HorizontalAlignment="Left" Margin="84,15,0,0" Name="txtPassword" VerticalAlignment="Top" Width="182" />
		<Button Content="Ok" Height="23" HorizontalAlignment="Left" Margin="191,51,0,0" Name="btnOk" VerticalAlignment="Top" Width="75" Click="btnOk_Click" IsDefault="True" />
		<Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="110,51,0,0" Name="btnCancel" VerticalAlignment="Top" Width="75" Click="btnCancel_Click" IsCancel="True" />
	</Grid>
</Window>
